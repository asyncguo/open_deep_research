{
  "name": "open-deep-research-js",
  "version": "1.0.0",
  "description": "TypeScript implementation of open deep research using LangGraph JS",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "devv": "tsx src/index.ts",
    "dev": "npx @langchain/langgraph-cli dev --port 2026 --config ./langgraph.json",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "langgraph",
    "typescript",
    "ai",
    "research",
    "agents"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/anthropic": "^0.3.7",
    "@langchain/community": "^0.3.21",
    "@langchain/core": "^0.3.66",
    "@langchain/deepseek": "^0.1.0",
    "@langchain/langgraph": "^0.2.49",
    "@langchain/langgraph-cli": "^0.0.52",
    "@langchain/openai": "^0.6.2",
    "axios": "^1.7.9",
    "dotenv": "^16.4.5",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}